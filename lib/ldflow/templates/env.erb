# Versions
ES_VERSION=<%= @options[:elasticsearch] %>
SS_VERSION=<%= @options[:superset] %>

# Passwords
ES_PASSWORD=<%= @elasticsearch_password %>
<%- if @options[:kibana] -%>
KIBANA_PASSWORD=<%= @kibana_password %>
<%- end -%>
SS_PASSWORD=<%= @superset_password %>

# Ports
ES_PORT=9200
<%- if @options[:kibana] -%>
KIBANA_PORT=5601
<%- end -%>
SS_PORT=8080

# Memory
ES_HEAP_INIT=128m
ES_HEAP_MAX=2g

# Container names
ES_CONTAINER_NAME=<%= Ldflow::Middleware::Container::ELASTICSEARCH %>
<%- if @options[:kibana] -%>
KIBANA_CONTAINER_NAME=<%= Ldflow::Middleware::Container::KIBANA %>
<%- end -%>
REDIS_CONTAINER_NAME=<%= Ldflow::Middleware::Container::SUPERSET_REDIS %>
DB_CONTAINER_NAME=<%= Ldflow::Middleware::Container::SUPERSET_DB %>
SS_CONTAINER_NAME=<%= Ldflow::Middleware::Container::SUPERSET %>
SS_INIT_CONTAINER_NAME=ldflow-superset-init
SS_WORKER_CONTAINER_NAME=<%= Ldflow::Middleware::Container::SUPERSET_WORKER %>
SS_WORKER_BEAT_CONTAINER_NAME=<%= Ldflow::Middleware::Container::SUPERSET_WORKER_BEAT %>
